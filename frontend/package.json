{
  "name": "student-request-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@hookform/resolvers": "5.2.2",
    "@reduxjs/toolkit": "2.3.0",
    "@tanstack/react-table": "8.21.3",
    "axios": "1.7.2",
    "bootstrap": "5.3.3",
    "bootstrap-icons": "1.13.1",
    "lodash": "4.17.21",
    "overlayscrollbars-react": "0.5.6",
    "react": "18.2.0",
    "react-bootstrap": "2.10.5",
    "react-bootstrap-daterangepicker": "8.0.0",
    "react-bootstrap-pagination-control": "1.0.5",
    "react-dom": "18.2.0",
    "react-hook-form": "7.68.0",
    "react-icons": "5.2.1",
    "react-redux": "9.1.2",
    "react-router-dom": "6.30.1",
    "react-scripts": "5.0.1",
    "react-toastify": "10.0.5",
    "redux": "5.0.1",
    "sass": "1.77.0",
    "socket.io-client": "4.8.1",
    "use-immer": "0.11.0",
    "zod": "4.2.1",
    "zustand": "5.0.9"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "imports": {
    "#custom-hooks": [
      "./src/custom-hooks/index-custom-hooks.js"
    ],
    "#custom-hooks/*": [
      "./src/custom-hooks/*"
    ],
    "#components": [
      "./src/components/index-components.js"
    ],
    "#components/*": [
      "./src/components/*"
    ],
    "#layouts/*": [
      "./src/layouts/*"
    ],
    "#pages/*": [
      "./src/pages/*"
    ],
    "#utils": [
      "./src/utils/index-utils.js"
    ],
    "#utils/*": [
      "./src/utils/*"
    ],
    "#services": [
      "./src/services/index-services.js"
    ],
    "#services/*": [
      "./src/services/*"
    ],
    "#schemas": [
      "./src/schemas/index-schemas.js"
    ],
    "#redux": [
      "./src/redux/index-redux.js"
    ],
    "#redux/*": [
      "./src/redux/*"
    ],
    "#assets/*": [
      "./src/assets/*"
    ],
    "#styles/*": [
      "./src/styles/*"
    ]
  }
}
